{
  "name": "plannify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "(cd client && yarn) && (cd server && yarn)",
    "client": "cd client && yarn start",
    "server": "cd server && yarn start",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "concurrently": "^4.1.0"
  }
}
